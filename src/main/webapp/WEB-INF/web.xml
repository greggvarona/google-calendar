<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>CalendarServlet</servlet-name>
        <servlet-class>jp.co.isr.gvarona.calendar.CalendarServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CalendarServlet</servlet-name>
        <url-pattern>/calendar</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CalendarAjaxServlet</servlet-name>
        <servlet-class>jp.co.isr.gvarona.calendar.CalendarAjaxServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CalendarAjaxServlet</servlet-name>
        <url-pattern>/calendar-ajax</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CalendarServletCallback</servlet-name>
        <servlet-class>jp.co.isr.gvarona.auth.AuthServletCallback</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CalendarServletCallback</servlet-name>
        <url-pattern>/calendar-callback</url-pattern>
    </servlet-mapping>


</web-app>
